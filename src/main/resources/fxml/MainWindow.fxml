<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TableView fx:id="tabParts" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1000.0">
         <columns>
            <TableColumn fx:id="colId" prefWidth="40.0" text="ID" />
            <TableColumn fx:id="colGroup" prefWidth="90.0" text="Group" />
            <TableColumn fx:id="colType" prefWidth="70.0" text="Type" />
            <TableColumn fx:id="colSubtype" prefWidth="70.0" text="Subtype" />
            <TableColumn fx:id="colName" prefWidth="90.0" text="Name" />
            <TableColumn fx:id="colDescription" prefWidth="370.0" text="Description" />
            <TableColumn fx:id="colSupplier" prefWidth="110.0" text="Supplier" />
            <TableColumn fx:id="colValue" prefWidth="160.0" text="Value" />
         </columns>
      </TableView>
   </center>
   <top>
      <ToolBar prefHeight="40.0" BorderPane.alignment="CENTER">
         <items>
            <Button mnemonicParsing="false" text="New item..." />
            <Separator prefWidth="100.0" />
            <Label text="Search" />
            <TextField />
            <Button mnemonicParsing="false" text="&gt;" textAlignment="CENTER" />
            <Button mnemonicParsing="false" text="..." />
         </items>
      </ToolBar>
   </top>
   <bottom>
      <HBox prefHeight="20.0" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
